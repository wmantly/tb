<!DOCTYPE html>
<html>
<head>
	<title>info</title>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
	<script src="/js/mustache.js"></script>
	<script src="http://ocean.718it.biz/jq-repeat/src/jq-repeat.js"></script>
	<script type="text/javascript">
	$( document ).ready( function(){
		$.get('./api/info', function( data ){
			console.log(data);
		} );
	} );
	</script>
</head>
<body>
	<table>
		<thead>
			<th>
				Type
			</th>
			<th>
				Name
			</th>
			<th>
				Size
			</th>
		</thead>
		<tbody>
			<tr jq-repeat="currentList">
				<td>
					{{{ type }}}
				</td>
				<td>
					{{{ name }}}
				</td>
				<td>
					{{{ size }}}
				</td>
			</tr>
		</tbody>
	</table>
</body>
</html>
